<Application x:Class="projet_location.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:projet_location"
             xmlns:converters="clr-namespace:projet_location.View.converter"
             StartupUri="View/FenetreConnexion.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View\styles\BasicButton.xaml"/>
                <ResourceDictionary Source="View\styles\AccessibleTabItem.xaml"/>
                <ResourceDictionary Source="View\styles\ModernTabItemStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <converters:ConvertisseurLargeurPadding x:Key="ConvertisseurLargeurPadding"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
